{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
        {
            "source": "/public/(.*)",
            "destination": "/partner/$1"
        },
        {
            "source": "/public",
            "destination": "/partner/index.html"
        },
        {
            "source": "/premium/(.*)",
            "destination": "/premium/$1"
        },
        {
            "source": "/premium",
            "destination": "/premium/index.html"
        },
        {
            "source": "/",
            "destination": "/premium/index.html"
        }
    ]
}